package com.hussainIT.thread;

import java.util.HashSet;
import java.util.Set;
import java.util.stream.Collectors;

public class Demo {
    public static void main(String[] args) {
    	 A a = new B();
         System.out.println(a.i);
         String str="GoodMorning";

         Set<Character> set=new HashSet<>();

         String collect = str.toLowerCase().chars()  // Convert to lowercase for case-insensitive processing
         .mapToObj(c -> (char) c)
         .filter(set::add)
         .map(String::valueOf)
         .collect(Collectors.joining());
         System.out.println(collect);
         
         str.toLowerCase().chars().filter(f->set.add((char) f)).map(String::valueOf).collect(Collectors.joining());

   
         

	}
}
class A
{
    int i = 10;
}
class B extends A
{
    int i = 20;
}


   
